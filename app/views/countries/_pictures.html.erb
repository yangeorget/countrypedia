<div class="row">
  <div class="col-md-8">
    <a href="#" class="thumbnail">
      <img class="img-responsive" src="<%= @country.googlemaps_url(maptype: 'hybrid', size: '1000x1000')  %>"/>
    </a>
  </div>
  <div class="col-md-4">
